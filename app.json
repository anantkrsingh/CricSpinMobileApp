{
  "expo": {
    "name": "CricSpin",
    "slug": "CricSpin",
    "version": "4.0",
    
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "stretch",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avssolution.fancylivecricketscore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.avssolution.fancylivecricketscore",
      "googleServicesFile": "./google-services.json",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "oneSignalAppId": "ff80ecf9-cf6a-4625-b51f-73e86bfac9d8",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.avssolution.fancylivecricketscore.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.avssolution.fancylivecricketscore.onesignal"
                    ]
                  }
                },
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.avssolution.fancylivecricketscore.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.avssolution.fancylivecricketscore.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "3fbeafff-96ba-47ce-93d3-d0594b87bd40"
      }
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ]
  }
}
